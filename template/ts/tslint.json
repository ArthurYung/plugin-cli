{
    "extends": ["tslint-config-standard"],
    "rules":{
    "ter-indent": 4,
    
    "printWidth": 100,
    
    "tabWidth": 4,
    
    "useTabs": false,
    
    "semi": true,
    
    "singleQuote": true,
    
    "jsxSingleQuote": false,
    
    "trailingComma": "none",
    
    "bracketSpacing": true,
    
    "jsxBracketSameLine": false,
    
    "arrowParens": "always",
    
    "rangeStart": 0,
    "rangeEnd": "Infinity",
    
    "requirePragma": false,
    
    "insertPragma": false,
    
    "proseWrap": "preserve",
    
    "endOfLine": "lf"
    }
}